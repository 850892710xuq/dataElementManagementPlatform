{
  "name": "data-element-platform",
  "version": "1.0.0",
  "description": "数据要素运营平台",
  "scripts": {
    "dev": "pnpm --filter \"./packages/**\" --parallel dev",
    "dev:main": "pnpm --filter ./packages/main run dev",
    "dev:data-manage": "pnpm --filter ./packages/data-manage run dev",
    "build": "pnpm --filter \"./packages/**\" build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write .",
    "clean": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.4.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "qiankun": "^2.10.13",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "esbuild": "0.18.20",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "mockjs": "^1.1.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.10",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  }
}